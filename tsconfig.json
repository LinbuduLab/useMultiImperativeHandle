{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "strict": true ,
    "esModuleInterop": true ,
    "skipLibCheck": true ,
    "forceConsistentCasingInFileNames": true ,
    "moduleResolution": "node",
    "lib": ["es6", "dom", "esnext"]
  },
  "include": ["src"],
  "exclude": ["./node_modules"]
}
